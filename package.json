{
  "name": "@archwayhq/cli",
  "version": "2.0.0",
  "description": "Develop WASM smart contracts with the Archway Network's Developer CLI",
  "homepage": "https://docs.archway.io",
  "repository": "github:archway-network/archway-cli",
  "keywords": [
    "archway",
    "atom",
    "blockchain",
    "cli",
    "cosmjs",
    "cosmos",
    "cosmosjs",
    "cosmwasm",
    "dapp",
    "dapps",
    "nft",
    "smart-contracts",
    "tendermint",
    "terminal",
    "tooling",
    "wasm",
    "web3"
  ],
  "author": "Archway Network (https://archway.io)",
  "contributors": [
    "Augusto ElesbÃ£o (https://github.com/aelesbao)",
    "Drew Taylor (https://github.com/drewstaylor)",
    "Elias Poroma (https://github.com/eliasmpw)"
  ],
  "license": "Apache-2.0",
  "bugs": "https://github.com/archway-network/archway-cli-v2/issues",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "bin": {
    "archway": "./bin/run"
  },
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "CHANGELOG.md",
    "LICENSE",
    "NOTICE",
    "README.md"
  ],
  "scripts": {
    "run:bin": "node ./bin/run",
    "run:dev": "node ./bin/dev",
    "clean": "shx rm -rf ./dist",
    "build": "yarn clean && tsc -b && tsc-alias -p tsconfig.json",
    "lint": "eslint --ext .js,.cjs,.ts --no-error-on-unmatched-pattern ./src",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha test/**/*.test.ts",
    "test:coverage": "nyc --reporter=text yarn test",
    "test:integration": "./test/integration/all.sh",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p conventionalcommits"
  },
  "dependencies": {
    "@archwayhq/arch3.js": "^0.4.0",
    "@archwayhq/keyring-go": "^0.1.1",
    "@cosmjs/amino": "^0.31.0",
    "@cosmjs/cosmwasm-stargate": "^0.31.1",
    "@cosmjs/crypto": "^0.31.0",
    "@cosmjs/encoding": "^0.31.0",
    "@cosmjs/ledger-amino": "^0.31.0",
    "@cosmjs/proto-signing": "^0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "@cosmjs/tendermint-rpc": "^0.31.0",
    "@ledgerhq/hw-transport-node-hid": "^6.27.18",
    "@oclif/core": "^2.8",
    "@oclif/plugin-autocomplete": "^2.3.10",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.1.1",
    "bip39": "^3.1.0",
    "chalk": "^4.1.2",
    "cosmjs-types": "^0.8.0",
    "debug": "^4.3.4",
    "dockerode": "^4.0.0",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "ow": "^0.28.2",
    "promisify-child-process": "^4.1.1",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "terminal-link": "^2.1.1",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@oclif/test": "^2.4",
    "@types/chai": "^4.3.4",
    "@types/debug": "^4.1.7",
    "@types/dockerode": "^3.3.16",
    "@types/elliptic": "^6.4.14",
    "@types/expect": "^24.3.0",
    "@types/ledgerhq__hw-transport": "^4.21.4",
    "@types/lodash": "^4.14.196",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.17.11",
    "@types/node-hid": "^1.3.1",
    "@types/prompts": "^2.4.4",
    "@types/semver": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "chai": "^4.3.7",
    "conventional-changelog-cli": "^4.1.0",
    "elliptic": "^6.5.4",
    "eslint": "^8.52.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.6",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-perfectionist": "^2.2.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^48.0.1",
    "fancy-test": "^2.0.18",
    "husky": "^8.0.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oclif": "^3.11.3",
    "shx": "^0.3.4",
    "sinon": "^15.0.3",
    "spawk": "^1.8.0",
    "ts-alias": "^0.0.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "oclif": {
    "additionalVersionFlags": [
      "version",
      "v",
      "-v"
    ],
    "bin": "archway",
    "dirname": "archway",
    "commands": "./dist/commands",
    "helpClass": "./dist/plugins/help/help",
    "plugins": [
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "accounts": {
        "description": "Manages a local keyring with wallets to sign transactions"
      },
      "accounts:balances": {
        "description": "Manage the token balance of an account"
      },
      "config": {
        "description": "Manage the configuration of the Archway CLI"
      },
      "config:chains": {
        "description": "Chain management commands, the chain files follow the Cosmos chain registry schema"
      },
      "contracts": {
        "description": "Perform actions with the contracts on a project"
      },
      "contracts:query": {
        "description": "Run queries against a contract"
      },
      "rewards": {
        "description": "Queries and transactions in the Archway network's Rewards module"
      }
    },
    "macos": {
      "identifier": "io.archway.cli"
    },
    "hooks": {
      "postrun": "./dist/hooks/postrun/versionCheck"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "os": [
    "!win32"
  ],
  "packageManager": "yarn@3.6.4"
}
